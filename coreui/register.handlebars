    <link rel="apple-touch-icon" sizes="76x76" href="assets/favicon/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="assets/favicon/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="assets/favicon/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="assets/favicon/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="assets/favicon/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="assets/favicon/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192" href="assets/favicon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="assets/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="assets/favicon/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="assets/favicon/favicon-16x16.png">
    <link rel="manifest" href="assets/favicon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="assets/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <!-- Vendors styles-->
    <link rel="stylesheet" href="vendors/simplebar/css/simplebar.css">
    <link rel="stylesheet" href="css/vendors/simplebar.css">
    <!-- Main styles for this application-->
    <link href="css/style.css" rel="stylesheet">
    <!-- We use those styles to show code examples, you should remove them in your application.-->
    <link href="css/examples.css" rel="stylesheet">
  </head>
  <body>
    <div class="bg-light min-vh-100 d-flex flex-row align-items-center">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-md-6">
            <div class="card mb-4 mx-4">
              <div class="card-body p-4">
                <form method="POST">
                <h1>Register</h1>
                <p class="text-medium-emphasis">Create your account</p>
                {{#if errors.usernameError}}
                        <div class="container" style="margin-bottom: 20px; color: red;">{{errors.usernameError}}</div>
                {{/if}}
                {{#if errors.userExistsError}}
                        <div class="container" style="margin-bottom: 20px; color: red;">{{errors.userExistsError}}</div>
                {{/if}}
                <div class="input-group mb-3"><span class="input-group-text">
                    <svg class="icon">
                      <use xlink:href="vendors/@coreui/icons/svg/free.svg#cil-user"></use>

                    </svg></span>
                  {{#if username}}
                    <input class="form-control" type="text" name="username" value="{{username}}">
                  {{else}}
                    <input class="form-control" type="text" name="username" placeholder="Username">
                  {{/if}}
                </div>
                {{#if errors.emailError}}
                        <div class="container" style="margin-bottom: 20px; color: red;">{{errors.emailError}}</div>
                {{/if}}
                <div class="input-group mb-3"><span class="input-group-text">
                    <svg class="icon">
                      <use xlink:href="vendors/@coreui/icons/svg/free.svg#cil-envelope-open"></use>
                    </svg></span>
                  {{#if email}}
                    <input class="form-control" name="email" type="text" value="{{email}}">
                  {{else}}
                    <input class="form-control" name="email" type="text" placeholder="Email">
                  {{/if}}
                </div>
                {{#if errors.passwordError}}
                        <div class="container" style="margin-bottom: 20px; color: red;">{{errors.passwordError}}</div>
                {{/if}}
                <div class="input-group mb-3"><span class="input-group-text">
                    <svg class="icon">
                      <use xlink:href="vendors/@coreui/icons/svg/free.svg#cil-lock-locked"></use>
                    </svg></span>
                  {{#if password}}
                    <input class="form-control" name="password" type="password" value="{{password}}">
                  {{else}}
                    <input class="form-control" name="password" type="password" placeholder="Password">
                  {{/if}}
                </div>
                {{#if errors.repeatError}}
                        <div class="container" style="margin-bottom: 20px; color: red;">{{errors.repeatError}}</div>
                {{/if}}
                <div class="input-group mb-4"><span class="input-group-text">
                    <svg class="icon">
                      <use xlink:href="vendors/@coreui/icons/svg/free.svg#cil-lock-locked"></use>
                    </svg></span>
                  <input class="form-control" type="password" name="repeatpassword" placeholder="Repeat password">
                </div>
                {{#if errors.matchError}}
                        <div class="container" style="margin-bottom: 20px; color: red;">{{errors.matchError}}</div>
                {{/if}}
                <button class="btn btn-block btn-success" type="submit">Create Account</button>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- CoreUI and necessary plugins-->
    <script src="vendors/@coreui/coreui/js/coreui.bundle.min.js"></script>
    <script src="vendors/simplebar/js/simplebar.min.js"></script>
    <script>
    </script>

  </body>
</html>